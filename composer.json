{
    "name": "pedrogaldino/php-proxy-server",
    "description": "Servidor de proxy com suporte a HTTP(S) implementado em PHP",
    "keywords": [
        "php",
        "proxy",
        "http-proxy",
        "https-proxy",
        "php-proxy"
    ],
    "homepage": "http://pedrogaldino.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Pedro Galdino",
            "email": "eu@pedrogaldino.com",
            "role": "developer"
        },
        {
            "name": "Rauny Henrique",
            "email": "rauny22@gmail.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "react/event-loop": "^1.1.1",
        "react/http": "^1.1",
        "guzzlehttp/guzzle": "~6.0",
        "tightenco/collect": "^7.3",
        "ext-json": "*",
        "clue/http-proxy-react": "^1.5"
    },
    "autoload": {
        "psr-4": { "Galdino\\Proxy\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Galdino\\Proxy\\Tests\\" : "test/" }
    },
    "require-dev": {
        "seregazhuk/php-watcher": "^0.5.2"
    }
}
